{
  "name": "M365 Copilot Usage Dashboard",
  "dataColors": [
    "#0078D4",
    "#107C10",
    "#FFB900",
    "#D83B01",
    "#6264A7",
    "#2B579A",
    "#217346",
    "#B7472A",
    "#7719AA",
    "#0F6CBD"
  ],
  "background": "#FAF9F8",
  "foreground": "#323130",
  "tableAccent": "#0078D4",
  "maximum": "#107C10",
  "center": "#FFB900",
  "minimum": "#D83B01",
  "good": "#107C10",
  "neutral": "#FFB900",
  "bad": "#D83B01",
  "textClasses": {
    "title": {
      "fontSize": 18,
      "fontFace": "Segoe UI Semibold",
      "color": "#323130"
    },
    "header": {
      "fontSize": 14,
      "fontFace": "Segoe UI Semibold",
      "color": "#323130"
    },
    "label": {
      "fontSize": 12,
      "fontFace": "Segoe UI",
      "color": "#605E5C"
    },
    "callout": {
      "fontSize": 28,
      "fontFace": "Segoe UI Light",
      "color": "#0078D4"
    },
    "largeLightTitle": {
      "fontSize": 32,
      "fontFace": "Segoe UI Light",
      "color": "#323130"
    }
  },
  "visualStyles": {
    "*": {
      "*": {
        "border": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "radius": 4
          }
        ],
        "background": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "title": [
          {
            "show": true,
            "fontSize": 12,
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            },
            "fontFamily": "Segoe UI Semibold",
            "alignment": "left"
          }
        ],
        "padding": [
          {
            "top": 8,
            "right": 8,
            "bottom": 8,
            "left": 8
          }
        ],
        "dropShadow": [
          {
            "show": true,
            "color": {
              "solid": {
                "color": "#000000"
              }
            },
            "position": "outer",
            "preset": "bottomRight",
            "transparency": 90,
            "angle": 45
          }
        ]
      }
    },
    "page": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#FAF9F8"
              }
            },
            "transparency": 0
          }
        ]
      }
    },
    "card": {
      "*": {
        "labels": [
          {
            "fontSize": 32,
            "fontFamily": "Segoe UI Light",
            "color": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ],
        "categoryLabels": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI",
            "color": {
              "solid": {
                "color": "#605E5C"
              }
            }
          }
        ],
        "wordWrap": [
          {
            "show": true
          }
        ]
      }
    },
    "multiRowCard": {
      "*": {
        "dataLabels": [
          {
            "fontSize": 14,
            "fontFamily": "Segoe UI Semibold",
            "color": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ],
        "categoryLabels": [
          {
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "color": {
              "solid": {
                "color": "#605E5C"
              }
            }
          }
        ],
        "cardTitle": [
          {
            "fontSize": 12,
            "fontFamily": "Segoe UI Semibold",
            "color": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ]
      }
    },
    "slicer": {
      "*": {
        "general": [
          {
            "outlineColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "outlineWeight": 1
          }
        ],
        "selection": [
          {
            "selectAllCheckboxEnabled": true,
            "singleSelect": false
          }
        ],
        "header": [
          {
            "show": true,
            "fontSize": 11,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ],
        "items": [
          {
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            },
            "background": {
              "solid": {
                "color": "#FFFFFF"
              }
            }
          }
        ]
      }
    },
    "lineChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": {
              "solid": {
                "color": "#0078D4"
              }
            },
            "showAllDataPoints": true
          }
        ],
        "lineStyles": [
          {
            "strokeWidth": 3,
            "lineStyle": "solid",
            "showMarker": true,
            "markerShape": "circle",
            "markerSize": 5
          }
        ],
        "labels": [
          {
            "show": false
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 10,
            "fontColor": {
              "solid": {
                "color": "#605E5C"
              }
            }
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 10,
            "fontColor": {
              "solid": {
                "color": "#605E5C"
              }
            },
            "gridlineShow": true,
            "gridlineColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            }
          }
        ]
      }
    },
    "barChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ],
        "labels": [
          {
            "show": true,
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "color": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "fontSize": 10,
            "fontColor": {
              "solid": {
                "color": "#605E5C"
              }
            }
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 10,
            "fontColor": {
              "solid": {
                "color": "#605E5C"
              }
            },
            "gridlineShow": true,
            "gridlineColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            }
          }
        ]
      }
    },
    "clusteredBarChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ]
      }
    },
    "columnChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ]
      }
    },
    "clusteredColumnChart": {
      "*": {
        "dataPoint": [
          {
            "defaultColor": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ]
      }
    },
    "donutChart": {
      "*": {
        "legend": [
          {
            "show": true,
            "position": "Right",
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ],
        "labels": [
          {
            "show": true,
            "labelStyle": "Both",
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "color": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ]
      }
    },
    "pieChart": {
      "*": {
        "legend": [
          {
            "show": true,
            "position": "Right",
            "fontSize": 10,
            "fontFamily": "Segoe UI"
          }
        ],
        "labels": [
          {
            "show": true,
            "labelStyle": "Both",
            "fontSize": 10
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "gridHorizontal": true,
            "gridHorizontalColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "rowPadding": 4,
            "outlineColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "outlineWeight": 1
          }
        ],
        "columnHeaders": [
          {
            "fontSize": 11,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "backColor": {
              "solid": {
                "color": "#0078D4"
              }
            },
            "alignment": "Left"
          }
        ],
        "values": [
          {
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            },
            "backColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            }
          }
        ],
        "total": [
          {
            "fontSize": 11,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            },
            "backColor": {
              "solid": {
                "color": "#F3F2F1"
              }
            }
          }
        ]
      }
    },
    "pivotTable": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            },
            "gridHorizontal": true,
            "gridHorizontalColor": {
              "solid": {
                "color": "#E1DFDD"
              }
            }
          }
        ],
        "columnHeaders": [
          {
            "fontSize": 11,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "backColor": {
              "solid": {
                "color": "#0078D4"
              }
            }
          }
        ],
        "rowHeaders": [
          {
            "fontSize": 10,
            "fontFamily": "Segoe UI Semibold",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ],
        "values": [
          {
            "fontSize": 10,
            "fontFamily": "Segoe UI",
            "fontColor": {
              "solid": {
                "color": "#323130"
              }
            }
          }
        ]
      }
    },
    "kpi": {
      "*": {
        "indicator": [
          {
            "show": true
          }
        ],
        "trendAxis": [
          {
            "show": true
          }
        ],
        "goals": [
          {
            "show": true,
            "fontSize": 10
          }
        ]
      }
    }
  }
}
